<!doctype html>
<html>
  <head>
    <meta charset="utf-8">

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="initial-scale=1">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= content_for?(:title) ? yield_content(:title) : 'Traject' %></title>

    <%= stylesheet_link_tag "normalize", "all" %>


    <link rel="apple-touch-icon-precomposed" sizes="152x152" href="apple-touch-icon-152x152-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="apple-touch-icon-144x144-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="apple-touch-icon-114x114-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="apple-touch-icon-72x72-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" href="apple-touch-icon-precomposed.png" />
    <link rel="shortcut icon" href="favicon.png" />
    <link rel="icon" type="image/ico" href="favicon.ico" />
  </head>



  <body class="<%= page_classes %>">
    <nav id="myScrollspy" class="navbar transparent navbar-fixed-top">
        <div class="container-fluid">
          <!-- Brand and toggle get grouped for better mobile display -->
          <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
              <span class="sr-only">Toggle navigation</span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="/">
              <img src="/images/Traject-logos/traject-icon-square-black-fill.svg" alt="Traject">
            </a>
          </div>

          <!-- Collect the nav links, forms, and other content for toggling -->
          <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
            <ul class="nav navbar-nav">
              <li><a href="#story">Big Picture</a></li>
              <li><a href="#action">Details</a></li>
              <li><a href="#blog">Articles</a></li>
              <li><a href="#contact">Contact</a></liL>
            </ul>
            <ul class="nav navbar-nav navbar-right">
                <!-- <li><a href="https://github.com/ashlynbaum"><i class="fa fa-github-square fa-2x"></i></a></li> -->
                <li><a href="https://twitter.com/Traject_NZ"><i class="fa fa-twitter-square fa-2x"></i></a></li>
                <li><a href="https://www.linkedin.com/company/traject-new-zealand"><i class="fa fa-linkedin-square fa-2x"></i></a></li>
            </ul>
          </div><!-- /.navbar-collapse -->
        </div><!-- /.container-fluid -->
      </nav>

    <!-- Jumbotron -->
    <section id="header">
      <div class="jumbotron">
        <div class="textbg">
          <div class="row">
            <div class="logo">
              <object data="images/Traject-logos/traject-logo-wtag-white-01.svg" type="image/svg+xml"></object>
            </div>
          </div>
          <div class="row">
            <div class="col-xs-12">
              <!-- <h2>Growing the knowledge economy</h2> -->
            </div>
          </div>
        </div>

      </div>
      <div class="row nextbtn">
        <p><a class="btn-circle" href="#story" role="button"><i class="fa fa-chevron-down fa-3x"></i></a></p>
      </div>
    </section>


    <!-- Traject -->
    <section id="story" class="projects">
      <div class="container-fluid">
        <div class="row heading">

          <!-- <div class="underline"></div> -->
        </div>
        <div class="story-text">
          <h2 class="left-quote">Traject gets skills recognised for New Zealand residence visas.</h2>
          <br>
          <h4 class="right-quote">'Cause <span class="underline-dashed">Skills</span> are more <span class="underline-dashed">relevant</span> than education in the tech sector today.</h4>
          <br>

        <br>
        </div>
          <div class="story-cartoon">
            <div class="flat-img">
              <object data="images/traject-color-story/technology.svg" type="image/svg+xml"></object>
              <h3 class="emph">Tech Migrants</h3>
            </div>
            <div class="arrows"> <i class="fa fa-arrow-right" aria-hidden="true"></i> </div>
            <div class="flat-img">
              <object data="images/traject-color-story/computer.svg" type="image/svg+xml"></object>
              <h3 class="emph">Get Assessed by Peers</h3>
            </div>
            <div class="arrows"> <i class="fa fa-arrow-right" aria-hidden="true"></i> </div>
            <div class="flat-img cert">
              <object data="images/traject-color-story/cert.svg" type="image/svg+xml"></object>
              <h3 class="emph">For A Residence Visa</h3>
            </div>
          </div>
          <div class="story-text">
            <br>
            <h4 class="left-quote">Weâ€™re making it possible for <span class="underline-dashed">migrants</span> who have skills, but no tertiary education, to become New Zealand <span class="underline-dashed">residents.</span> </h4>
          </div>
      </div>
    </section>

    <section id="action">
      <div id="action-parallax">
        <div class="row heading">
         <h3>We're currently <span class="underline-dashed">prototyping</span>.</h3>
         <h2>Want to be part of it?</h2>
        </div>
        <div class="action-selectors">
          <div class="action migrant hvr-grow hvr-underline-reveal on">
            <div class="action-icon">
              <!-- <i class="fa fa-user" aria-hidden="true"></i> -->
              <object id="migrant-img" data="images/traject-icons/traject-user.svg" type="image/svg+xml"></object>
            </div>
            <h3 class="action-desc">Get Skills Assessed</h3>
          </div>
          <div class="action accredited hvr-grow hvr-underline-reveal">
            <div class="action-icon">
              <!-- <i class="fa fa-university" aria-hidden="true"></i> -->
              <object id="accredited-img" data="images/traject-icons/traject-building.svg" type="image/svg+xml"></object>
            </div>
            <h3 class="action-desc">Become Skill Assessor</h3>
          </div>
        </div>
        <div>
        </div>
        <div class="assessment-panel">
          <div class="migrant-text">
            <div class="title">
              <h2>Migrants</h2>
            </div>
            <div class="content">
              <div class="form">
                <form action="https://formspree.io/info@traject.nz"
                      method="POST"
                      class="migrant-email">
                    <h3 class="request"><i class="fa fa-share fa-3x" aria-hidden="true"></i>Help us make change, let's talk:</h3>
                    <div class="email-submit">
                      <div class="input-field col-md-4">
                        <input type="text" name="name" class="validate">
                        <label for="name">Name</label>
                    </div>
                    <div class="input-field col-md-4">
                      <input type="email" name="_replyto">
                      <label for="_replyto">Email</label>
                    </div>
                    <div class="input-field col-md-3">
                      <input type="submit" value="Get More Info" class="btn wave-effect">
                    </div>
                  </div>
                </form>
              </div>
              <div class="migrant-info">


              </div>
            </div>
          </div>
          <div class="accredited-text">
          <div class="title">
            <h2>Assessors</h2>
          </div>
          <div class="content">
            <h3>Coming Soon</h3>
            <p>We're still negotiating with Immigration as to how exactly the Assessments will be conducted. Who's doing them, what the framework looks like, how quality can be assured, etc. If you'd like to be involved in developing this service with us, please get in touch.</p>
          </div>
          </div>
        </div>
      </div>
    </section>

    <!-- blog -->
    <section id="blog">
      <%= yield %>
    </section>



    <!-- Contact -->
    <section id="contact">
      <div class="container-fluid">
        <div class="row heading">
          <h2>Contact</h2>
          <div class="underline"></div>
        </div>
        <div class="row v-align">
          <div class="col-md-5 col-md-push-6">
            <h3 class="header">Connect:</h3>
            <ul class="list-unstyled">
              <li><i class="fa fa-envelope-o"></i> : <a target="_blank" href="mailto:info@traject.nz"> info@traject.nz</a></li>
            </ul>
            <h3>Co-Founders:</h3>
            <ul class="list-unstyled">
              <li><i class="fa fa-envelope-o"></i> : <a target="_blank" href="mailto:info@traject.nz"> connor@traject.nz</a></li>
              <li><i class="fa fa-envelope-o"></i> : <a target="_blank" href="mailto:info@traject.nz"> ashlyn@traject.nz</a></li>
            </ul>
            <h3 class="header"> Location:</h3>
            <ul class="list-unstyled">
              <li>Wellington, New Zealand</li>
            </ul>
          </div>
          <div class="col-md-4 col-md-offset-2 col-md-pull-5">
              <img src="/images/traject-logo-01-01.svg" alt="traject">
          </div>
        </div>
      </div>
    </section>

    <section id="footer">
      <div class="container-fluid">
        <div class="row">
          <p>
            <a href="https://twitter.com/Traject_NZ"><i class="fa fa-twitter-square fa-2x"></i></a>
            <a href="https://www.linkedin.com/company/traject-new-zealand"><i class="fa fa-linkedin-square fa-2x"></i></a>
          </p>
        </div>
      </div>
    </section>
    <%= google_analytics_universal_tag %>
  </body>
  <%= javascript_include_tag  "all" %>
  <!-- Compiled and minified JavaScript -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.6/js/materialize.min.js"></script>


</html>
